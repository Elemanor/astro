const warningSignsStructural = [
  {
    title: "Foundation Cracks",
    description: "Horizontal, vertical, or step cracks in foundation walls",
    icon: "alert-triangle"
  },
  {
    title: "Wall Separation",
    description: "Gaps where walls meet ceilings or other walls",
    icon: "zap"
  },
  {
    title: "Floor Problems",
    description: "Uneven, sloping, or cracked floors throughout home",
    icon: "trending"
  },
  {
    title: "Door/Window Issues",
    description: "Sticking, gaps, or misalignment in frames",
    icon: "shield"
  }
];

const warningSignsEnvironmental = [
  {
    title: "Water Infiltration",
    description: "Basement leaks or persistent dampness",
    icon: "droplets"
  },
  {
    title: "Settling Noises",
    description: "Creaking or popping sounds from structure",
    icon: "volume"
  },
  {
    title: "Exterior Signs",
    description: "Brick cracks, bulging walls, or chimney lean",
    icon: "home"
  },
  {
    title: "Landscape Changes",
    description: "Soil movement or water pooling near foundation",
    icon: "map"
  }
];

const serviceAreas = [
  {
    title: "Toronto Proper",
    locations: [
      "Downtown Toronto: Condos and heritage homes",
      "North York: Residential and commercial projects",
      "Scarborough: Suburban foundation reinforcement",
      "Etobicoke: Clay soil underpinning specialists"
    ]
  },
  {
    title: "GTA Communities",
    locations: [
      "Mississauga: New and existing construction",
      "Brampton: Expanding home foundations",
      "Markham: Modern home underpinning",
