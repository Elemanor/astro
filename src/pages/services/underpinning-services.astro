      { item: "Structural drawings", description: "$2,000-$5,000" },
      { item: "Building permits", description: "$1,500-$3,000" },
      { item: "Engineering oversight", description: "$3,000-$8,000" },
      { item: "City inspections", description: "Built into permit costs" }
